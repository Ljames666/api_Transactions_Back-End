{
    "version": 2,

    "builds": [
        {
            "buildCommand": "npm run build",
            "src": "dist/main.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}
